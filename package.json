		"astro": "^5.9.2",
		"tailwindcss": "^4.1.8",
		"typescript": "^5.7.3"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"astro-meta-tags": "^0.3.1"
	}
}
