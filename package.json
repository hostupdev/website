{
	"name": "hostup-website",
	"type": "module",
	"version": "1.0.3",
	"scripts": {
		"astro": "astro",
		"dev": "astro dev",
		"start": "astro dev",
		"build": "astro check && astro build",
		"preview": "astro preview",
		"compose": "docker compose -f compose.yaml up --build",
		"recompose": "docker compose -f compose.yaml up --build --force-recreate",
		"format": "biome format --write .",
		"lint": "biome lint --fix .",
		"check": "biome check --write ."
	},
	"dependencies": {
		"@astrojs/check": "^0.9.4",
		"@astrojs/node": "^9.3.0",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "4.1.11",
		"astro": "^5.12.0",
		"tailwindcss": "^4.1.11",
		"typescript": "^5.7.3"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"astro-meta-tags": "^0.4.0"
	}
}
