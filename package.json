{
	"name": "website",
	"type": "module",
	"version": "1.0.0",
	"scripts": {
		"astro": "astro",
		"dev": "astro dev",
		"start": "astro dev",
		"build": "astro check && astro build",
		"preview": "astro preview",
		"compose": "docker compose --env-file .env.dev -f docker-compose.dev.yaml up --build",
